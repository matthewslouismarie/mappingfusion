RewriteEngine On
RewriteRule ^public\/(.*)$ public/$1 [QSA,L]
RewriteRule ^(.*)$ index.php?route_params=$1 [QSA,L]

ExpireActive On
ExpiresDefault "access plus 31536000 seconds"
ExpiresByType text/html "access plus 2 seconds"

<FilesMatch "^[^\.]+$">
    Header set Cache-Control "must-revalidate,  max-age=120"
</FilesMatch>

<FilesMatch "\.(js)|(svg)|(webp)">
    Header set Cache-Control "max-age=31536000, immutable"
</FilesMatch>

#RewriteEngine On
#RewriteRule "^$" "index.php?route_params=home"
#RewriteRule "^public\/(.*)$" "public/$1" [END]
#RewriteRule "^(.*)$" "index.php?route_params=$1"